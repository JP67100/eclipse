package tableau41;

public class App {

	public static void main(String[] args) {
		
		int tab [] = {0,0,0,0,0,0,0};
		   // Déclaration du tableau d'entiers
        int[] tabl = new int[7];

        // Remplissage du tableau avec des valeurs zéro
        for (int i = 0; i < tab.length; i++) {
            tab[i] = 0;
        }
        for (int valeur : tab) {
            System.out.println(valeur);
        

	}}}

